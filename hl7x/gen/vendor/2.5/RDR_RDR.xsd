    <xsd:complexType name="RDR_RDR.CONTENT">
        <xsd:sequence>
            <xsd:element ref="MSH" minOccurs="1" maxOccurs="1"/>
            <xsd:element ref="MSA" minOccurs="1" maxOccurs="1"/>
